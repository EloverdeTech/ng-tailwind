{
  "name": "ng-tailwind-documentation",
  "version": "0.0.9",
  "scripts": {
    "ng-high-memory": "node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng",
    "ng": "ng",
    "start": "eslint . --ext .ts && ngtw build -c ng-tailwind-config.js && ng serve",
    "build": "eslint . --ext .ts && ng build ng-tailwind && ngtw build -p -c ng-tailwind-config.js && ng build --configuration=production",
    "build-ngt": "ng build ng-tailwind --configuration=production",
    "test": "ng test",
    "lint": "eslint . --ext .ts",
    "fix-lint": "eslint . --ext .ts --fix",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/core": "^14.2.11",
    "@angular/animations": "^14.3.0",
    "@angular/common": "^14.3.0",
    "@angular/compiler": "^14.3.0",
    "@angular/core": "^14.3.0",
    "@angular/forms": "^14.3.0",
    "@angular/platform-browser": "^14.3.0",
    "@angular/platform-browser-dynamic": "^14.3.0",
    "@angular/router": "^14.3.0",
    "@ng-select/ng-select": "^10.0.3",
    "angular-svg-icon": "12.0.0",
    "eloverde-doc-viewer": "^1.1.0",
    "flatpickr": "4.6.3",
    "inputmask": "^4.0.9",
    "moment": "^2.24.0",
    "ng2-flatpickr": "^9.0.0",
    "ngx-dropzone": "^2.5.0",
    "rxjs": "~6.6.7",
    "tailwindcss": "^2.2.19",
    "tslib": "^2.0.0",
    "viewerjs": "^1.9.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.11",
    "@angular/cli": "^14.2.11",
    "@angular/compiler-cli": "^14.3.0",
    "@angular/language-service": "^14.3.0",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "2.31.0",
    "@typescript-eslint/parser": "2.31.0",
    "autoprefixer": "^10.2.5",
    "codelyzer": "^6.0.0",
    "eslint": "^6.8.0",
    "ng-packagr": "^14.2.2",
    "postcss": "^8.3.0",
    "protractor": "~7.0.0",
    "ts-node": "~7.0.0",
    "typescript": "~4.6.4"
  }
}