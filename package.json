{
  "name": "ng-tailwind-documentation",
  "version": "0.0.9",
  "scripts": {
    "ng-high-memory": "node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng",
    "ng": "ng",
    "start": "eslint . --ext .ts && ngtw build -c ng-tailwind-config.js && ng serve",
    "build": "eslint . --ext .ts && ng build ng-tailwind && ngtw build -p -c ng-tailwind-config.js && ng build --configuration=production",
    "build-ngt": "ng build ng-tailwind --configuration=production",
    "test": "ng test",
    "lint": "eslint . --ext .ts",
    "fix-lint": "eslint . --ext .ts --fix",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/core": "^18.2.15",
    "@angular/animations": "^18.2.13",
    "@angular/common": "^18.2.13",
    "@angular/compiler": "^18.2.13",
    "@angular/core": "^18.2.13",
    "@angular/forms": "^18.2.13",
    "@angular/platform-browser": "^18.2.13",
    "@angular/platform-browser-dynamic": "^18.2.13",
    "@angular/router": "^18.2.13",
    "@ng-select/ng-select": "^13.0.0",
    "angular-svg-icon": "^18.0.0",
    "eloverde-doc-viewer": "2.0.0",
    "ev-date-picker": "^1.1.0",
    "inputmask": "^5.0.8",
    "moment": "^2.29.4",
    "ngx-dropzone": "^3.1.0",
    "rxjs": "^7.8.2",
    "tailwindcss": "^2.2.19",
    "tslib": "^2.8.1",
    "viewerjs": "^1.11.7",
    "zone.js": "^0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.2.15",
    "@angular/cli": "^18.2.15",
    "@angular/compiler-cli": "^18.2.13",
    "@angular/language-service": "^18.2.13",
    "@types/node": "^22.13.10",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "autoprefixer": "^10.4.21",
    "codelyzer": "^6.0.2",
    "eslint": "^8.49.0",
    "ng-packagr": "^18.2.1",
    "ng-tailwindcss": "^2.3.1",
    "postcss": "^8.5.3",
    "ts-node": "^10.9.2",
    "typescript": "5.4.5"
  }
}